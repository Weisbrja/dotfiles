\definecolor{gruvboxbackground}{HTML}{282828}       % #282828
\definecolor{gruvboxforeground}{HTML}{ebdbb2}       % #ebdbb2
\definecolor{gruvboxbackgroundbright}{HTML}{3c3836} % #3c3836
\definecolor{gruvboxwhite}{HTML}{a89984}            % #a89984

\definecolor{gruvboxcyan}{HTML}{689d6a}             % #689d6a
\definecolor{gruvboxblue}{HTML}{458588}             % #458588
\definecolor{gruvboxbluebright}{HTML}{83a598}       % #83a598
\definecolor{gruvboxgreen}{HTML}{98971a}            % #98971a
\definecolor{gruvboxred}{HTML}{cc241d}              % #cc241d

\mode<presentation>

\setbeamercolor*{normal text}{fg=gruvboxforeground,bg=gruvboxbackground}
\setbeamercolor*{structure}{fg=gruvboxcyan,bg=gruvboxbackground}
\setbeamercolor*{example text}{fg=gruvboxblue,bg=gruvboxbackground}
\setbeamercolor*{alerted text}{fg=gruvboxred,bg=gruvboxbackground}

\setbeamercolor{frametitle}{fg=gruvboxforeground,bg=gruvboxbackground}
\setbeamercolor{author}{fg=gruvboxforeground,bg=gruvboxbackground}
\setbeamercolor{date}{fg=gruvboxforeground,bg=gruvboxbackground}
\setbeamercolor{institute}{fg=gruvboxforeground,bg=gruvboxbackground}

\setbeamercolor*{item}{fg=gruvboxforeground,bg=gruvboxbackground}
\setbeamercolor*{section in toc}{fg=gruvboxforeground,bg=gruvboxbackground}

\setbeamercolor*{navigation symbols}{fg=gruvboxwhite,bg=gruvboxbackground}
\setbeamercolor*{navigation symbols dimmed}{fg=gruvboxbackgroundbright,bg=gruvboxbackground}

\mode<all>
