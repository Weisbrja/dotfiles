\definecolor{gruvboxbackground}{HTML}{282828}
\definecolor{gruvboxforeground}{HTML}{ebdbb2}
\definecolor{gruvboxwhite}{HTML}{a89984}
\definecolor{gruvboxbrightblack}{HTML}{928374}

\definecolor{gruvboxgreen}{HTML}{98971a}
\definecolor{gruvboxred}{HTML}{cc241d}
\definecolor{gruvboxpurple}{HTML}{b16286}
\definecolor{gruvboxlightblue}{HTML}{83a598}

\mode<presentation>

\setbeamercolor*{palette primary}{fg=gruvboxpurple,bg=gruvboxbackground}
\setbeamercolor*{normal text}{use=palette primary,fg=gruvboxforeground,bg=palette primary.bg}
\setbeamercolor*{structure}{use=palette primary,fg=gruvboxpurple,bg=palette primary.bg}
\setbeamercolor*{example text}{use=structure,fg=gruvboxlightblue,bg=structure.bg}
\setbeamercolor*{alerted text}{use=structure,fg=gruvboxred,bg=structure.bg}

\setbeamercolor{frametitle}{use=palette primary}
\setbeamercolor{author}{use=palette primary,fg=gruvboxlightblue}
\setbeamercolor{date}{use=palette primary,fg=gruvboxgreen}
\setbeamercolor{institute}{use=palette primary,fg=gruvboxred}

\setbeamercolor*{item}{fg=gruvboxgreen}
\setbeamercolor*{section in toc}{fg=gruvboxlightblue}

\setbeamercolor*{navigation symbols}{fg=gruvboxwhite}
\setbeamercolor*{navigation symbols dimmed}{fg=gruvboxbrightblack}

\mode<all>
